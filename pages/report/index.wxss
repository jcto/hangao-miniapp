.table-box{
  width: 686rpx;
  margin: 0 auto;
  padding: 20rpx;
  box-sizing: border-box;
  /* background-color: #fff; */
}
.wraper{
  border: 1rpx solid #ccc;
  border-radius: 8rpx;
  overflow: hidden;
  background-color: #fff;
}
.th{
  box-sizing: border-box;
  background-color: #B72729;
  height: 70rpx;
  line-height: 70rpx;
  /* padding: 10rpx 0rpx; */
  width: 100%;
  font-size: 26rpx;
  text-align: center;
  color: #fff;
}
.td{
  box-sizing: border-box;
  background-color: #fff;
  height: 60rpx;
  line-height: 60rpx;
  /* padding: 10rpx 0rpx; */
  width: 100%;
  font-size: 24rpx;
  text-align: center;
  color: #333;
}
.section{
  background-color: #fff;

  color: #333;
  padding: 20rpx;
}
.sli{
  border-bottom: 1rpx solid #ccc;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
}
.site-img{
  height: 44rpx;
  width: 44rpx;
  display: inline-block;
  position: relative;
  margin-bottom: -11rpx;
}
