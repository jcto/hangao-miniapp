
<van-popup custom-class='my-pop'  closeable 
close-icon-position="bottom"
show="{{ show }}" bind:close="onClose">
<view style="height: 400rpx; width:200rpx;background-color: rgba(0, 0, 0, 0);">
<view style="height: 300rpx; width:200rpx;"></view>
</view>

</van-popup>

<van-tabbar inactive-color='#000' active-color='#B72729' active="{{ active }}" bind:change="onChange">
    <van-tabbar-item>
        <image slot="icon" src="/imgs/home2.png" mode="aspectFit" style="width: 30rpx; height: 18rpx;" />
        <image slot="icon-active" src="/imgs/home.png" mode="aspectFit" style="width: 60rpx; height: 40rpx;" />
        首页/ Home</van-tabbar-item>
    <van-tabbar-item>
        <image src="/imgs/user.png" style="width: 30rpx; height: 18rpx;"/>
    </van-tabbar-item>
    <van-tabbar-item>
        <image slot="icon" src="/imgs/user.png" mode="aspectFit" style="width: 120rpx; height: 100rpx;" />
        <image slot="icon-active" src="/imgs/user2.png" mode="aspectFit" style="width: 120rpx; height: 100rpx;" />
        个人/Personal</van-tabbar-item>
</van-tabbar>
