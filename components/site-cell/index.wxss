
.user-cell {
    position: relative;
    display: flex;
    display: -webkit-flex;
    width: 648rpx;
    height: 120rpx;
    margin:0 auto;
    margin-top: 40rpx;
    padding-bottom: 40rpx;
  }
  .cell-content{
    width: 100%;
  }
  .start-cell-content {
    border-bottom: 2rpx solid #e2e2e2;
    width: 100%;
    height: 72rpx;
    line-height: 72rpx;
  }
  .end-cell-content {
    border-bottom: 2rpx solid #e2e2e2;
    width: 100%;
    height: 72rpx;
    line-height: 72rpx;
  }
  button.user-cell-button {
    background: none;
    border: 0rpx;
    text-align: left;
    border-bottom: 2rpx solid #e2e2e2;
    padding: 0;
    display: block;
    width: 100%;
  }
  /* button.user-cell-button::after {
    border: none;
  } */
  .cell-label {
    font-size: 30rpx;
    color: #333;
  }
  .cell-value {
    font-size: 26rpx;
    color: #333;
  }
  .cell-icon {
    height: 130rpx;
    width: 44rpx;
    display: block;
    margin-left: 6rpx;
margin-right: 14rpx;
margin-top: 8rpx;

    /* margin-right: 10rpx;
    margin-top: -10rpx; */
  }
  